{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AICAGE tool definition",
  "type": "object",
  "required": [
    "tool_path",
    "tool_full_name",
    "tool_homepage"
  ],
  "properties": {
    "tool_path": {
      "type": "string"
    },
    "tool_full_name": {
      "type": "string"
    },
    "tool_homepage": {
      "type": "string"
    },
    "base_exclude": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "base_distro_exclude": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false
}
